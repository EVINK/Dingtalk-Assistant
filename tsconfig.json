{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./src/js/",
    "sourceMap": true,
    "removeComments": true,
    "lib": [
      "es2016",
      "dom"
    ],
  },
  "include": [
    "./src/ts/*.ts",
    "src/ts/types/*.d.ts"
  ],
  "exclude": [
    "node_modules",
  ]
}